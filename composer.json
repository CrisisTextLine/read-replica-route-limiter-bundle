{
    "name": "cjcodes/slave-route-limiter-bundle",
    "description": "Instead of globally applying a master-slave, this bundle, when enabled, requires devs to flag routes as @ShouldUseSlave to use the slave database, otherwise it defaults to master.",
    "type": "symfony-bundle",
    "require": {
        "symfony/framework-bundle": "^2.3",
        "doctrine/orm": "^2.4.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.3"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Chris Johnson",
            "email": "chris@cjcodes.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "CJCodes\\SlaveRouteLimiterBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "helpers\\": "Tests/helpers"
        }
    }
}
